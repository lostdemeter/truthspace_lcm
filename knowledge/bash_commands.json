{
  "metadata": {
    "name": "bash_commands",
    "version": "1.0.0",
    "description": "Core bash command knowledge for TruthSpace",
    "domain": "PROGRAMMING"
  },
  "entries": [
    {
      "name": "ls",
      "type": "COMMAND",
      "description": "List directory contents",
      "keywords": ["ls", "list", "files", "directory", "folder", "contents", "show"],
      "output_type": "bash",
      "syntax": "ls [options] [path]",
      "examples": ["ls", "ls -la", "ls -lh /tmp", "ls *.py"],
      "flags": {
        "-l": "long format with details",
        "-a": "show hidden files",
        "-h": "human readable sizes",
        "-R": "recursive listing",
        "-t": "sort by time",
        "-S": "sort by size"
      }
    },
    {
      "name": "cd",
      "type": "COMMAND",
      "description": "Change current directory",
      "keywords": ["cd", "change", "directory", "navigate", "go", "folder"],
      "output_type": "bash",
      "syntax": "cd [path]",
      "examples": ["cd /home", "cd ..", "cd ~", "cd -"]
    },
    {
      "name": "pwd",
      "type": "COMMAND",
      "description": "Print current working directory",
      "keywords": ["pwd", "current", "directory", "where", "path", "location"],
      "output_type": "bash",
      "syntax": "pwd",
      "examples": ["pwd"]
    },
    {
      "name": "mkdir",
      "type": "COMMAND",
      "description": "Create directories",
      "keywords": ["mkdir", "create", "make", "directory", "folder", "new"],
      "output_type": "bash",
      "syntax": "mkdir [options] directory",
      "examples": ["mkdir mydir", "mkdir -p path/to/dir", "mkdir -m 755 secure_dir"],
      "flags": {
        "-p": "create parent directories as needed",
        "-m": "set permissions mode",
        "-v": "verbose output"
      }
    },
    {
      "name": "rmdir",
      "type": "COMMAND",
      "description": "Remove empty directories",
      "keywords": ["rmdir", "remove", "delete", "directory", "folder", "empty"],
      "output_type": "bash",
      "syntax": "rmdir [options] directory",
      "examples": ["rmdir emptydir", "rmdir -p path/to/empty"]
    },
    {
      "name": "rm",
      "type": "COMMAND",
      "description": "Remove files or directories",
      "keywords": ["rm", "remove", "delete", "file", "directory", "erase"],
      "output_type": "bash",
      "syntax": "rm [options] file",
      "examples": ["rm file.txt", "rm -r directory", "rm -f force.txt", "rm -rf dir"],
      "flags": {
        "-r": "recursive (for directories)",
        "-f": "force without prompting",
        "-i": "interactive mode",
        "-v": "verbose output"
      }
    },
    {
      "name": "cp",
      "type": "COMMAND",
      "description": "Copy files and directories",
      "keywords": ["cp", "copy", "duplicate", "backup", "clone", "to"],
      "output_type": "bash",
      "syntax": "cp [options] source destination",
      "examples": ["cp file.txt backup.txt", "cp -r dir1 dir2", "cp *.py /backup/"],
      "flags": {
        "-r": "recursive (for directories)",
        "-i": "interactive mode",
        "-v": "verbose output",
        "-p": "preserve attributes"
      }
    },
    {
      "name": "mv",
      "type": "COMMAND",
      "description": "Move or rename files and directories",
      "keywords": ["mv", "move", "rename", "relocate", "to"],
      "output_type": "bash",
      "syntax": "mv [options] source destination",
      "examples": ["mv old.txt new.txt", "mv file.txt /other/dir/", "mv dir1 dir2"],
      "flags": {
        "-i": "interactive mode",
        "-v": "verbose output",
        "-f": "force without prompting"
      }
    },
    {
      "name": "touch",
      "type": "COMMAND",
      "description": "Create empty file or update timestamp",
      "keywords": ["touch", "create", "file", "empty", "new", "timestamp"],
      "output_type": "bash",
      "syntax": "touch [options] file",
      "examples": ["touch newfile.txt", "touch -t 202312150000 file.txt"]
    },
    {
      "name": "cat",
      "type": "COMMAND",
      "description": "Concatenate and display file contents",
      "keywords": ["cat", "view", "display", "show", "contents", "file", "read", "print"],
      "output_type": "bash",
      "syntax": "cat [options] file",
      "examples": ["cat file.txt", "cat file1.txt file2.txt", "cat -n file.txt"],
      "flags": {
        "-n": "number lines",
        "-b": "number non-blank lines",
        "-s": "squeeze blank lines"
      }
    },
    {
      "name": "head",
      "type": "COMMAND",
      "description": "Display first lines of file",
      "keywords": ["head", "first", "top", "beginning", "lines", "file"],
      "output_type": "bash",
      "syntax": "head [options] file",
      "examples": ["head file.txt", "head -n 20 file.txt", "head -c 100 file.txt"],
      "flags": {
        "-n": "number of lines",
        "-c": "number of bytes"
      }
    },
    {
      "name": "tail",
      "type": "COMMAND",
      "description": "Display last lines of file",
      "keywords": ["tail", "last", "end", "bottom", "final", "follow", "-f"],
      "output_type": "bash",
      "syntax": "tail [options] file",
      "examples": ["tail file.txt", "tail -n 20 file.txt", "tail -f logfile.log"],
      "flags": {
        "-n": "number of lines",
        "-f": "follow file changes",
        "-c": "number of bytes"
      }
    },
    {
      "name": "grep",
      "type": "COMMAND",
      "description": "Search for patterns in files",
      "keywords": ["grep", "search", "pattern", "text", "match", "filter", "error", "string"],
      "output_type": "bash",
      "syntax": "grep [options] pattern [file]",
      "examples": ["grep 'error' log.txt", "grep -r 'TODO' .", "grep -i 'hello' file.txt", "grep -n 'pattern' file.txt"],
      "flags": {
        "-i": "case insensitive",
        "-r": "recursive search",
        "-n": "show line numbers",
        "-v": "invert match",
        "-c": "count matches",
        "-l": "list files only"
      }
    },
    {
      "name": "find",
      "type": "COMMAND",
      "description": "Find files in directory hierarchy",
      "keywords": ["find", "search", "locate", "files", "directory", "name"],
      "output_type": "bash",
      "syntax": "find [path] [expression]",
      "examples": ["find . -name '*.py'", "find /tmp -type f", "find . -mtime -7", "find . -size +1M"],
      "flags": {
        "-name": "match filename pattern",
        "-type": "file type (f=file, d=directory)",
        "-mtime": "modification time in days",
        "-size": "file size",
        "-exec": "execute command on results"
      }
    },
    {
      "name": "chmod",
      "type": "COMMAND",
      "description": "Change file permissions",
      "keywords": ["chmod", "permission", "executable", "+x", "mode", "access", "runnable"],
      "output_type": "bash",
      "syntax": "chmod [options] mode file",
      "examples": ["chmod +x script.sh", "chmod 755 file", "chmod -R 644 dir/"],
      "flags": {
        "-R": "recursive",
        "+x": "add execute permission",
        "-x": "remove execute permission"
      }
    },
    {
      "name": "chown",
      "type": "COMMAND",
      "description": "Change file owner and group",
      "keywords": ["chown", "owner", "ownership", "user", "group"],
      "output_type": "bash",
      "syntax": "chown [options] owner[:group] file",
      "examples": ["chown user file.txt", "chown user:group file.txt", "chown -R user dir/"]
    },
    {
      "name": "echo",
      "type": "COMMAND",
      "description": "Display text or variables",
      "keywords": ["echo", "print", "display", "output", "text", "variable"],
      "output_type": "bash",
      "syntax": "echo [options] [string]",
      "examples": ["echo 'Hello'", "echo $PATH", "echo -n 'no newline'", "echo -e 'line1\\nline2'"],
      "flags": {
        "-n": "no trailing newline",
        "-e": "enable escape sequences"
      }
    },
    {
      "name": "wc",
      "type": "COMMAND",
      "description": "Count lines, words, and characters",
      "keywords": ["wc", "count", "lines", "words", "characters", "bytes"],
      "output_type": "bash",
      "syntax": "wc [options] file",
      "examples": ["wc file.txt", "wc -l file.txt", "wc -w file.txt"],
      "flags": {
        "-l": "count lines",
        "-w": "count words",
        "-c": "count bytes",
        "-m": "count characters"
      }
    },
    {
      "name": "sort",
      "type": "COMMAND",
      "description": "Sort lines of text",
      "keywords": ["sort", "order", "arrange", "lines", "alphabetical", "numeric"],
      "output_type": "bash",
      "syntax": "sort [options] file",
      "examples": ["sort file.txt", "sort -n numbers.txt", "sort -r file.txt", "sort -u file.txt"],
      "flags": {
        "-n": "numeric sort",
        "-r": "reverse order",
        "-u": "unique lines only",
        "-k": "sort by field"
      }
    },
    {
      "name": "uniq",
      "type": "COMMAND",
      "description": "Filter adjacent duplicate lines",
      "keywords": ["uniq", "unique", "distinct", "duplicate", "deduplicate"],
      "output_type": "bash",
      "syntax": "uniq [options] [input [output]]",
      "examples": ["uniq file.txt", "sort file.txt | uniq", "uniq -c file.txt"],
      "flags": {
        "-c": "count occurrences",
        "-d": "only show duplicates",
        "-u": "only show unique"
      }
    },
    {
      "name": "tar",
      "type": "COMMAND",
      "description": "Archive files",
      "keywords": ["tar", "archive", "compress", "extract", "backup", "tarball"],
      "output_type": "bash",
      "syntax": "tar [options] [archive] [files]",
      "examples": ["tar -cvf archive.tar dir/", "tar -xvf archive.tar", "tar -czvf archive.tar.gz dir/", "tar -xzvf archive.tar.gz"],
      "flags": {
        "-c": "create archive",
        "-x": "extract archive",
        "-v": "verbose",
        "-f": "specify filename",
        "-z": "gzip compression",
        "-j": "bzip2 compression"
      }
    },
    {
      "name": "gzip",
      "type": "COMMAND",
      "description": "Compress files with gzip",
      "keywords": ["gzip", "compress", "zip", "shrink"],
      "output_type": "bash",
      "syntax": "gzip [options] file",
      "examples": ["gzip file.txt", "gzip -k file.txt", "gzip -d file.txt.gz"],
      "flags": {
        "-k": "keep original file",
        "-d": "decompress",
        "-v": "verbose"
      }
    },
    {
      "name": "curl",
      "type": "COMMAND",
      "description": "Transfer data from URLs",
      "keywords": ["curl", "download", "http", "https", "url", "api", "fetch", "web", "request"],
      "output_type": "bash",
      "syntax": "curl [options] url",
      "examples": ["curl https://example.com", "curl -o file.html https://example.com", "curl -X POST -d 'data' url"],
      "flags": {
        "-o": "output to file",
        "-O": "save with remote filename",
        "-X": "HTTP method",
        "-d": "POST data",
        "-H": "add header",
        "-s": "silent mode"
      }
    },
    {
      "name": "wget",
      "type": "COMMAND",
      "description": "Download files from web",
      "keywords": ["wget", "download", "http", "url", "fetch", "web", "file"],
      "output_type": "bash",
      "syntax": "wget [options] url",
      "examples": ["wget https://example.com/file.zip", "wget -O output.html https://example.com"],
      "flags": {
        "-O": "output filename",
        "-q": "quiet mode",
        "-c": "continue partial download",
        "-r": "recursive download"
      }
    },
    {
      "name": "ps",
      "type": "COMMAND",
      "description": "List running processes",
      "keywords": ["ps", "process", "running", "list", "pid"],
      "output_type": "bash",
      "syntax": "ps [options]",
      "examples": ["ps", "ps aux", "ps -ef", "ps -p 1234"],
      "flags": {
        "aux": "all processes with details",
        "-ef": "full format listing",
        "-p": "specific PID"
      }
    },
    {
      "name": "kill",
      "type": "COMMAND",
      "description": "Terminate processes",
      "keywords": ["kill", "terminate", "stop", "process", "signal", "pid"],
      "output_type": "bash",
      "syntax": "kill [options] pid",
      "examples": ["kill 1234", "kill -9 1234", "kill -SIGTERM 1234"],
      "flags": {
        "-9": "force kill (SIGKILL)",
        "-15": "graceful termination (SIGTERM)"
      }
    },
    {
      "name": "df",
      "type": "COMMAND",
      "description": "Display disk space usage",
      "keywords": ["df", "disk", "space", "free", "filesystem", "storage", "mounted"],
      "output_type": "bash",
      "syntax": "df [options] [path]",
      "examples": ["df", "df -h", "df -h /home"],
      "flags": {
        "-h": "human readable sizes",
        "-T": "show filesystem type"
      }
    },
    {
      "name": "du",
      "type": "COMMAND",
      "description": "Estimate file space usage",
      "keywords": ["du", "disk", "usage", "size", "directory", "space"],
      "output_type": "bash",
      "syntax": "du [options] [path]",
      "examples": ["du -sh .", "du -h --max-depth=1", "du -sh *"],
      "flags": {
        "-s": "summary only",
        "-h": "human readable",
        "--max-depth": "limit directory depth"
      }
    },
    {
      "name": "ssh",
      "type": "COMMAND",
      "description": "Secure shell remote login",
      "keywords": ["ssh", "remote", "login", "shell", "terminal", "into", "connect"],
      "output_type": "bash",
      "syntax": "ssh [options] user@host",
      "examples": ["ssh user@server.com", "ssh -p 2222 user@host", "ssh -i key.pem user@host"],
      "flags": {
        "-p": "port number",
        "-i": "identity file (key)",
        "-v": "verbose mode"
      }
    },
    {
      "name": "scp",
      "type": "COMMAND",
      "description": "Secure copy files over SSH",
      "keywords": ["scp", "copy", "remote", "transfer", "ssh", "file"],
      "output_type": "bash",
      "syntax": "scp [options] source destination",
      "examples": ["scp file.txt user@host:/path/", "scp user@host:/path/file.txt .", "scp -r dir/ user@host:/path/"],
      "flags": {
        "-r": "recursive (directories)",
        "-P": "port number",
        "-i": "identity file"
      }
    }
  ]
}
